<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="225.0" maxWidth="500.0" minHeight="225.0" minWidth="500.0" prefHeight="225.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.nbp.controller.CreateNewUserController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="FirstName" prefWidth="100.0" text="FirstName:" />
            <TextField fx:id="InputFirstNameTextField" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="LastNameLabel" prefWidth="100.0" text="LastName:" />
            <TextField fx:id="InputLastNameTextField" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="PasswordLabel" prefWidth="100.0" text="Password:" />
            <PasswordField fx:id="InputPasswordField" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="RepeatPasswordLabel" prefWidth="100.0" text="Repeat password:" />
            <PasswordField fx:id="InputRepeatPasswordField" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="150.0">
         <children>
            <Button fx:id="CreateNewUserButton" mnemonicParsing="false" text="Create New User" />
            <Button fx:id="BackToUserListButton" mnemonicParsing="false" text="Back To User List" />
         </children>
         <padding>
            <Insets left="50.0" />
         </padding>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
