<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="225.0" maxWidth="500.0" minHeight="225.0" minWidth="500.0" prefHeight="225.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.nbp.controller.UserListController">
   <children>
      <Pane prefHeight="50.0" prefWidth="500.0">
         <children>
            <Label fx:id="titleLabel" alignment="CENTER" prefHeight="50.0" prefWidth="500.0" text="NBP Currency Getter" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS" />
         </children>
      </Pane>
      <HBox prefHeight="75.0" prefWidth="500.0" spacing="10.0">
         <children>
            <Label fx:id="loginLabel" prefWidth="80.0" text="Login:" />
            <ComboBox fx:id="userComboBox" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="500.0" spacing="10.0">
         <children>
            <Label fx:id="passwordLabel" contentDisplay="CENTER" prefWidth="80.0" text="Password:" textAlignment="CENTER" />
            <PasswordField fx:id="passwordField" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="500.0" spacing="200.0">
         <children>
            <Button fx:id="loginButton" mnemonicParsing="false" text="Log In">
               <HBox.margin>
                  <Insets left="90.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="createUserButton" mnemonicParsing="false" text="Create New User" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
